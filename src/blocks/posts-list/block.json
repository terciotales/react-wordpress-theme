{
    "$schema" : "https://schemas.wp.org/trunk/block.json",
    "apiVersion" : 2,
    "name" : "react-wordpress-theme/posts-list",
    "title" : "Listagem de Posts",
    "category" : "react-wordpress-theme",
    "version" : "1.0.0",
    "description" : "",
    "textdomain" : "react-wordpress-theme",
    "attributes" : {
        "categories" : {
            "type" : "array",
            "default" : []
        },
        "taxonomies" : {
            "type" : "array",
            "default" : []
        },
        "postIn" : {
            "type" : "array",
            "default" : []
        },
        "postNotIn" : {
            "type" : "array",
            "default" : []
        },
        "postType" : {
            "type" : "string",
            "default" : "post"
        },
        "perPage" : {
            "type" : "number",
            "default" : 10
        },
        "offset" : {
            "type" : "number",
            "default" : 0
        },
        "order" : {
            "type" : "string",
            "default" : "desc"
        },
        "orderBy" : {
            "type" : "string",
            "default" : "date"
        },
        "metaQuery" : {
            "type" : "object",
            "default" : {}
        },
        "search" : {
            "type" : "string",
            "default" : ""
        },
        "status" : {
            "type" : "array",
            "default" : ["publish"]
        },
        "blogId" : {
            "type" : "number",
            "default" : 1
        },
        "pagination" : {
            "type" : "boolean",
            "default" : false
        },
        "paginationPrevText" : {
            "type" : "string",
            "default" : "«"
        },
        "paginationEllipsis" : {
            "type" : "string",
            "default" : "..."
        },
        "paginationNextText" : {
            "type" : "string",
            "default" : "»"
        },
        "paginationFirstText" : {
            "type" : "string",
            "default" : "Primeira"
        },
        "paginationLastText" : {
            "type" : "string",
            "default" : "Última"
        }
    },
    "supports" : {
        "spacing" : {
            "blockGap" : true,
            "margin" : true,
            "padding" : true,
            "units" : [
                "px",
                "rem",
                "%"
            ]
        }
    },
    "style" : "react-wordpress-theme-style-posts-list",
    "script" : "react-wordpress-theme-script-posts-list",
    "editorStyle" : "react-wordpress-theme-style-editor-posts-list"
}
